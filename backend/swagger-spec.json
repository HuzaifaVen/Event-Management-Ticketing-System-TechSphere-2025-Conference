      "SignUpDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Full name of the user",
            "example": "Abdullah Ahmed"
          },
          "email": {
            "type": "string",
            "description": "User's email address (must be unique)",
            "example": "abdullah12@gmail.com"
          },
          "password": {
            "type": "string",
            "description": "Password should be atleast 8 digits long",
            "minLength": 8,
            "example": "StrongPass123"
          },
          "role": {
            "type": "string",
            "description": "Role to assign to the user",
            "enum": [
              "organizer",
              "customer",
              "admin"
            ],
            "example": "customer"
          }
        },
        "required": [
          "name",
          "email",
          "password",
          "role"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "abdullah12@gmail.com"
          },
          "password": {
            "type": "string",
            "description": "plaw12!@"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "VerifyLoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Enter Email: "
          },
          "otp": {
            "type": "string",
            "description": "Enter OTP: "
          }
        },
        "required": [
          "email",
          "otp"
        ]
      },
      "ChangePasswordDto": {
        "type": "object",
